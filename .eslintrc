{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-unused-vars": 0,
    "max-len": [1, 120, 2, {"ignoreComments": true}],
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "class-methods-use-this": 0
  },
  "extends": ["airbnb-base"],
  "env": {
    "browser": true,
    "node": true
  }
}